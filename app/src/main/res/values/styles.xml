<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.Full" parent="AppTheme.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="button_style">
        <item name="android:textColor">#000000</item>
        <item name="android:background">@drawable/anniu</item>
    </style>
    <style name="button_buy">
        <item name="android:background">@drawable/button_buy2</item>
    </style>
    <style name="edit_1">
        <item name="android:background">@drawable/edit1</item>
    </style>
    <style name="button_query">
        <item name="android:background">@drawable/button_query</item>
    </style>
    <style name="list1">
        <item name="android:background">@drawable/list1</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="button_add">
        <item name="android:background">@drawable/button_add</item>
    </style>
    <style name="button_sub">
        <item name="android:background">@drawable/button_sub</item>
    </style>
    <style name="button_orange">
        <item name="android:background">@drawable/button_orange</item>
    </style>
    <style name="button_green">
        <item name="android:background">@drawable/button_green</item>
    </style>
    <style name="button_blue">
        <item name="android:background">@drawable/button_blue</item>
    </style>
    <style name="button_qing">
        <item name="android:background">@drawable/button_qing</item>
    </style>

</resources>
